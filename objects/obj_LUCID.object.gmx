        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Selection and Selecting


//New Game

if selectedcontrol = 0
{
    obj_New_Game.selected = 1;
}
else
{
    obj_New_Game.selected = 0;
}

//Select New Game
// Test

if can_select = 1
{
if obj_New_Game.selected = 1 and Key_A and keyboard_check(ord('V'))
{
    room_goto(room_1);
    instance_create(168,112,obj_Oenn_Body);
    instance_create(168,112,obj_Oenn_Legs);
    //instance_create(x,y,obj_MusicBox);
    audio_play_sound(snd_Select,10,0);
}
// Intro
if obj_New_Game.selected = 1 and Key_A 
and !keyboard_check(ord('I'))
and !keyboard_check(ord('V'))
and !keyboard_check(ord('O'))
{
    
    room_goto(room_Prologue);
    global.prologuenext = 1;
    instance_create(x,y,obj_room_fade_in);
    /*
    room_goto(room_IntroStage_00);
    instance_create(x,y,obj_room_fade_in);
    with instance_create(596,334,obj_Oenn_Body)
    {
        state = states.sitting;
        enterfirst = 1;
    }
    instance_create(596,334,obj_Oenn_Legs)
    audio_play_sound(snd_Select,10,0);
    */
}

// Tutorials
if (obj_New_Game.selected = 1 and Key_A  and keyboard_check(ord('O'))
and !keyboard_check(ord('V')) and !keyboard_check(ord('I')))
or Key_LBump
{
    room_goto(room_Tutorial_1);
    with instance_create(96,48,obj_Oenn_Body)
    {
        enterfirst = 1;
    }
    instance_create(96,48,obj_Oenn_Legs);
    instance_create(96,48,obj_HUD);
    audio_play_sound(snd_Select,10,0);
    instance_create(x,y,obj_MusicBox); // Music Box
}

// Intro Stage
if (obj_New_Game.selected = 1 and Key_A  and keyboard_check(ord('I'))
and !keyboard_check(ord('V')))
or Key_RBump
{
    room_goto(room_IntroStage_1);
    with instance_create(80,32,obj_Oenn_Body)
    {
        enterfirst = 1;
    }
    instance_create(80,32,obj_Oenn_Legs);
    instance_create(80,32,obj_HUD);
    audio_play_sound(snd_Select,10,0);
    instance_create(x,y,obj_MusicBox); // Music Box
}
}


//Continue

if selectedcontrol = 1
{
    obj_Continue.selected = 1;
}
else
{
    obj_Continue.selected = 0;
}

//Select Continue

if can_select = 1
{
if obj_Continue.selected = 1 and Key_A
{
    script_execute(Open_Saved);
    if file_exists("save.ini")
    {
    var roomgo;
    roomgo = ini_read_real("Oenn Room","roomlocation",0);
    room_goto(roomgo);
    instance_create(global.x,global.y,obj_Oenn_Body);
    instance_create(global.x,global.y,obj_Oenn_Legs);
    instance_create(x,y,obj_HUD);
    instance_create(x,y,obj_MusicBox); // Music Box
    }
    ini_close();
}
}

//Exit

if selectedcontrol = 2
{
    obj_Exit.selected = 1;
}
else
{
    obj_Exit.selected = 0;
}

//Select Exit

if can_select = 1
{
if obj_Exit.selected = 1 and Key_A
{
    game_end();
}
}
</string>
          </argument>
