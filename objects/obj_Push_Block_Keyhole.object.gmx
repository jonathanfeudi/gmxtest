/*
///Physics

Key_Left    =   -(gamepad_button_check(global.cp,gp_padl) or keyboard_check(vk_left));
Key_Right   =   gamepad_button_check(global.cp,gp_padr) or keyboard_check(vk_right);


//Gravity

if global.slowmo = 1
{
    if (vsp &lt; 5) vsp += grav;
}
if global.slowmo = 2
{
    if (vsp &lt; 5) vsp += grav/4;
}

// Collisions

//Horizontal Collision

if round(place_meeting((x+hsp),(y),par_wall))
{
    while round(!place_meeting((x+sign(hsp)),(y),par_wall))
    {
        x += sign(hsp);
    }
    
    hsp = 0;
}

x += floor(hsp);


//Vertical Collision

if round(place_meeting((x),round(y+vsp),par_wall))
{
    while round(!place_meeting((x),(y+sign(vsp)),par_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

//Vertical Collision PLATFORM

if round(place_meeting((x),round(y+vsp),par_Platform))
{
    while round(!place_meeting((x),(y+sign(vsp)),par_Platform))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

y += vsp;


//PUSH

if  x &gt; obj_Oenn_Body.x and obj_Oenn_Body.state = states.pushing 
and distance_to_object(obj_Oenn_Body) &lt; 8 and Key_Right
{
    hsp = 2;
}


if x &lt; obj_Oenn_Body.x and obj_Oenn_Body.state = states.pushing
and distance_to_object(obj_Oenn_Body) &lt; 8 and -Key_Left
{
    hsp = -2;
}

if obj_Oenn_Body.state != states.pushing 
or (!place_meeting(x,y+1,par_wall) and !place_meeting(x,y+1,par_Platform))
{
    hsp = 0;
}

if vsp &gt; 0
{
    obj_Oenn_Body.pushable = 0;
}
else
{
    obj_Oenn_Body.pushable = 1;
}

