        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Switching States

switch (enemy_state)
{
    //// Underground
    case enemy_states.enemy_underground:     
    sprite_index = spr_Enemy_Oremole_Underground;
    mask_index = -1;   
       
    // Oenn close enough
    if distance_to_object(obj_Oenn_Body) &lt; 160 
    { 
        image_index = 0;
        enemy_state = enemy_states.enemy_up;
    }
    break;
    
    //// Up
    case enemy_states.enemy_up:
    sprite_index = spr_Enemy_Oremole_Up;
    if image_index = 4
    {
        audio_play_sound(snd_Oremole_Up,10,0);
    }
    mask_index = -1; 
    break;
    
    //// Idle
    case enemy_states.enemy_idle:
    sprite_index = spr_Enemy_Oremole_Idle;
    
    // Go Down
    if distance_to_object(obj_Oenn_Body) &gt; 200
    {
        image_index = 0;
        enemy_state = enemy_states.enemy_down;
    }
    // Start Shooting
    if !collision_line(x,y,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,false)
    {
        if distance_to_object(obj_Oenn_Body) &lt; 140
            {
                //image_index = 0;
                enemy_state = enemy_states.enemy_shooting;
            }
    }
    break;
    
    //// Down
    case enemy_states.enemy_down: 
    sprite_index = spr_Enemy_Oremole_Down;
    if image_index = 4
    {
        audio_play_sound(snd_Oremole_Down,10,0);
    }

    break;
    
    // Shooting
    case enemy_states.enemy_shooting:
    sprite_index = spr_Enemy_Oremole_Attack;
    if image_index = 8
    {
        if image_xscale &lt; 0
        {
            with instance_create(x,y,obj_Oremole_Shot)
                {
                    direction = point_direction(x,y,obj_Oenn_Body.x,obj_Oenn_Body.y);
                    image_angle = direction;
                }
        }
        if image_xscale &gt; 0
        {
            with instance_create(x,y,obj_Oremole_Shot)
                {
                    direction = point_direction(x,y,obj_Oenn_Body.x,obj_Oenn_Body.y)
                    image_angle = direction;
                }
        }
    }
    
    // Idle
    if image_index &gt;  8
    {
    if distance_to_object(obj_Oenn_Body) &gt; 140
    or collision_line(x,y,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,false)
    {
    {
        //image_index = 0;
        enemy_state = enemy_states.enemy_idle;
    }
    }
    }
    break;
    
    
    
    // Damaged
    case enemy_states.enemy_damaged:
    sprite_index = spr_Enemy_Oremole_Damaged;
    break;
    
    // Death
    case enemy_states.enemy_death:
    sprite_index = spr_Enemy_Oremole_Death;
    break;
}

</string>
          </argument>
