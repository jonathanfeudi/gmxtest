        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Animation Ends

// Idle to Walking

if sprite_index = spr_BTA_Grunt_Idle
{
    walk_counter += 1;
}

// Alerted to Running

if sprite_index = spr_BTA_Grunt_Alerted
{
    enemy_state = enemy_states.enemy_running;
    cb_alerted = 0;
}
    
    
// Melee

if sprite_index = spr_BTA_Grunt_Slash
{
    enemy_state = enemy_states.enemy_idle;
    alarm[0] = room_speed*4;
}

// Jumping

if sprite_index = spr_BTA_Grunt_Jump
{
    image_speed = 0;
    enemy_state = enemy_states.enemy_landing;
}

// Landing

if sprite_index = spr_BTA_Grunt_Landing
{
    dir = 4;
    enemy_state = enemy_states.enemy_idle;
    alarm[0] = room_speed*4;
}

// Damaged

if sprite_index = spr_BTA_Grunt_DMG
{
    enemy_state = enemy_states.enemy_idle;
    alarm[0] = room_speed*4;
    cb_alerted = 0;
}

// Damaged 2

if sprite_index = spr_BTA_Grunt_DMG2
{
    dmg2 = 0;
    enemy_state = enemy_states.enemy_idle;
    alarm[0] = room_speed*4;
    cb_alerted = 0;
}

// Death

if sprite_index = spr_BTA_Grunt_Death
{ 
    instance_create(x,y-16,obj_Enemy_DeathSmoke);
    // Death Burn Carcass
    if image_xscale = 1
    {
        with instance_create(x,y,obj_Enemy_Cinder_DeathBurn)
        {
            image_xscale = 1;
        }
    }
    if image_xscale = -1
    {
        with instance_create(x,y,obj_Enemy_Cinder_DeathBurn)
        {
            image_xscale = -1;
        }
    }       
    audio_play_sound(snd_Enemy_Death,10,0);
    instance_create(x,y-16,obj_Shard_Small);
    if random(100) &lt; 75 then  // Chances for Health
    {
        instance_create(x,y-16,obj_Health_Small);
    }
    instance_destroy();
}
</string>
