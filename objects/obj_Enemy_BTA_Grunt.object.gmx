        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Switching States

switch (enemy_state)
{
    
    /// Alerted
    case enemy_states.enemy_alerted:
    sprite_index = spr_BTA_Grunt_Alerted;
    hsp = 0;
    can_melee = 1;
    animation = 1/2;
    // Direction
    if x &gt; obj_Oenn_Body.x
    {
        dir = -4;
    }
    else
    {
        dir = 4;
    }
    
    scr_Enemie_CinderGrav();
       
    break;
    
    //// Idle
    case enemy_states.enemy_idle: 
    sprite_index = spr_BTA_Grunt_Idle;
    hsp = 0;
    can_melee = 1;
    animation = 1/4;
    
    // Strike
    if place_meeting(x,y+1,par_wall) and distance_to_object(obj_Oenn_Body) &lt; strike//16
    and ((y - obj_Oenn_Body.y) &lt; 16)
    {
        image_index = 0;
        enemy_state = enemy_states.enemy_melee;
    }
    // Jump Strike
    if place_meeting(x,y+1,par_Wall_Floor) and distance_to_object(obj_Oenn_Body) &lt; strike
    and ((y - obj_Oenn_Body.y) &gt;= 16)
    {
        up_counter += 1;
        if up_counter &gt;= 45
        {
            up_counter = 0;
            image_index = 0;
            enemy_state = enemy_states.enemy_up;
        }
    }
    
    // Alerted
    if  cb_alerted = 1
    {
        if ((position_meeting(x+((sprite_width/4)*sign(-dir)), y+(sprite_height/4)+8,par_wall) and (x &lt; obj_Oenn_Body.x))
        or ((position_meeting(x+((sprite_width/4)*sign(dir)), y+(sprite_height/4)+8,par_wall)) and (x &gt; obj_Oenn_Body.x)))
        or (place_meeting(x,y+2,obj_slope) and (x &lt; obj_Oenn_Body.x))// Works with Slopes
        or (place_meeting(x,y+2,obj_slope) and (x &gt; obj_Oenn_Body.x))
        {
            if can_run = 1 and ((x &gt; obj_Oenn_Body.x + 4) or (x &lt; obj_Oenn_Body.x - 4)) 
            and bored &lt; 100
            {
                if ((collision_line(x,y-16,x-vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true))
                or (collision_line(x,y-16,x+vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true)))
                or 
                COV.on = 1 and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y-16,par_Wall_Floor,false,true)
                {
                    image_index = 0;
                    enemy_state = enemy_states.enemy_alerted;
                    audio_play_sound(snd_Cinder_Alert,10,0);
                    //cb_alerted = 0;
                    dirMAX = 4;  // Used to remember dir after Crystal Hold
                    dir = 4;
                }
            }
        }
    }
    
    // Start Running
    if  cb_alerted = 0
    {
        if ((position_meeting(x+((sprite_width/4)*sign(-dir)), y+(sprite_height/4)+8,par_wall) and (x &lt; obj_Oenn_Body.x))
        or ((position_meeting(x+((sprite_width/4)*sign(dir)), y+(sprite_height/4)+8,par_wall)) and (x &gt; obj_Oenn_Body.x)))
        {
            if can_run = 1 and ((x &gt; obj_Oenn_Body.x + 4) or (x &lt; obj_Oenn_Body.x - 4)) 
            and bored &lt; 100
            {
                if ((collision_line(x,y-16,x-vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true))
                or (collision_line(x,y-16,x+vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true)))
                or 
                COV.on = 1 and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y-16,par_wall,false,true)
                {
                    image_index = 0;
                    enemy_state = enemy_states.enemy_running;
                    dirMAX = 4;  // Used to remember dir after Crystal Hold
                    dir = 4;
                }
            }
        }
    }
    
    scr_Enemie_CinderGrav();
    
    break;
    
    //// Walk
    case enemy_states.enemy_walking: 
    sprite_index = spr_BTA_Grunt_Walk;
    //dirMAX = 2;  // Used to remember dir after Crystal Hold
    //dir = 2;
    hsp = dir/global.slowmo;
    can_melee = 1;
    walk_counter = 0;
    animation = 1/4;
    //cb_alerted = 1;
    
    if (place_meeting((x+hsp),(y),par_Wall_Floor))
    or (place_meeting((x+hsp),(y),obj_Crystal_Wall))
    { 
        dir *= -1;
    }
    
    // Alerted
    if  cb_alerted = 1
    {
        if ((position_meeting(x+((sprite_width/4)*sign(-dir)), y+(sprite_height/4)+8,par_wall) and (x &lt; obj_Oenn_Body.x))
        or ((position_meeting(x+((sprite_width/4)*sign(dir)), y+(sprite_height/4)+8,par_wall)) and (x &gt; obj_Oenn_Body.x)))
        or (place_meeting(x,y+2,obj_slope) and (x &lt; obj_Oenn_Body.x))
        or (place_meeting(x,y+2,obj_slope) and (x &gt; obj_Oenn_Body.x))
        {
            if can_run = 1 and ((x &gt; obj_Oenn_Body.x + 4) or (x &lt; obj_Oenn_Body.x - 4)) 
            and bored &lt; 100
            {
                if ((collision_line(x,y-16,x-vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true))
                or (collision_line(x,y-16,x+vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true)))
                or 
                COV.on = 1 and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y-16,par_Wall_Floor,false,true)
                {
                    image_index = 0;
                    enemy_state = enemy_states.enemy_alerted;
                    audio_play_sound(snd_Cinder_Alert,10,0);
                   //cb_alerted = 0;
                    dirMAX = 4;  // Used to remember dir after Crystal Hold
                    dir = 4;
                }
            }
        }
    }
    
    // Start Running
    if  cb_alerted = 0
        {
        if ((position_meeting(x+((sprite_width/4)*sign(-dir)), y+(sprite_height/4)+8,par_Wall_Floor) and (x &lt; obj_Oenn_Body.x))
        or ((position_meeting(x+((sprite_width/4)*sign(dir)), y+(sprite_height/4)+8,par_Wall_Floor)) and (x &gt; obj_Oenn_Body.x)))
        {
            if can_run = 1 and ((x &gt; obj_Oenn_Body.x + 4) or (x &lt; obj_Oenn_Body.x - 4)) 
            and bored &lt; 100
            {
                if ((collision_line(x,y-16,x-vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true))
                or (collision_line(x,y-16,x+vision,y,obj_Oenn_Body,false,true)
                and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y,par_wall,false,true)))
                or 
                COV.on = 1 and !collision_line(x,y-16,obj_Oenn_Body.x,obj_Oenn_Body.y-16,par_wall,false,true)
                {
                    image_index = 0;
                    enemy_state = enemy_states.enemy_running;
                    dirMAX = 4;  // Used to remember dir after Crystal Hold
                    dir = 4;
                }
            }
        }
    }
    
    scr_Enemie_CinderGrav();
        
    break;
    
    //// Running
    case enemy_states.enemy_running: 
    sprite_index = spr_BTA_Grunt_Run;
    hsp = dir/global.slowmo;
    up_counter = 0;
    walk_counter = 0;
    vision = 128;
    animation = 1/2;
    // Direction
    if x &gt; obj_Oenn_Body.x
    {
        dir = -4;
    }
    else
    {
        dir = 4;
    }
    
    // Stop if Can't See
    
    if collision_line(x,y-32,obj_Oenn_Body.x,obj_Oenn_Body.y-16,par_Wall_Floor,false,true)
    {
        image_index = 0;
        enemy_state = enemy_states.enemy_idle;
        alarm[0] = room_speed*4;
    }
    
    // Strike
    if place_meeting(x,y+1,par_wall) and distance_to_object(obj_Oenn_Body) &lt; strike
    {
        image_index = 0;
        enemy_state = enemy_states.enemy_melee;
    }
    
    // Revert to Idles
    
    if (x &lt; obj_Oenn_Body.x + 4) and (x &gt; obj_Oenn_Body.x - 4) 
    {
        image_index = 0;
        enemy_state = enemy_states.enemy_idle;
        alarm[0] = room_speed*4;
    }
    
    // Jumping
    
    if (place_meeting(x+1,y,par_Wall_Floor) and x &lt; obj_Oenn_Body.x)
    or (place_meeting(x-1,y,par_Wall_Floor) and x &gt; obj_Oenn_Body.x)
    //and distance_to_object(obj_Oenn_Body) &lt; vision
    {
        enemy_state = enemy_states.enemy_jumping;
    }
    scr_Enemie_CinderGrav();

    break;
    
    //// Jumping
    case enemy_states.enemy_jumping: 
    sprite_index = spr_BTA_Grunt_Jump;
    hsp = dir/global.slowmo;
    animation = 1/4;
    
    if place_meeting(x,y+1,par_Wall_Floor) and image_index &gt;= 7
    and image_index &lt;= 8
    {
        vsp = (-jumpspeed)/global.slowmo; // jumps up
        dir /= 2;
    }
    if vsp &gt; 0 and place_meeting(x,y+1,par_wall)
    {
        enemy_state = enemy_states.enemy_landing;
    }
    
    scr_Enemie_CinderGrav();
    
    break;
    
    //// Up
    case enemy_states.enemy_up: 
    sprite_index = spr_BTA_Grunt_Jump;
    animation = 1/8;
    
    //hsp = dir/global.slowmo;
    if place_meeting(x,y+1,par_Wall_Floor) and image_index &gt;= 7
    {
        vsp = (-jumpspeed)/global.slowmo; // jumps up
        //dir /= 2;
    }
    if vsp &gt; 0 and place_meeting(x,y+2,par_Wall_Floor)
    {
        enemy_state = enemy_states.enemy_landing;
    }
    
    scr_Enemie_CinderGrav();
    
    break;
    
    //// Landing
    case enemy_states.enemy_landing: 
    animation = 1/4;
    
    if place_meeting(x,y+1,par_Wall_Floor)
    {
        sprite_index = spr_BTA_Grunt_Landing; 
    }
    hsp = 0;
    
    scr_Enemie_CinderGrav();
    
    break;
    
    // Melee
    case enemy_states.enemy_melee: 
