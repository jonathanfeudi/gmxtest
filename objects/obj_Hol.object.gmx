        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Switching States

switch (state)
{
    // Sitting
    case "sitting": 
    sprite_index = spr_Hol_Sitting;
    image_speed = 1/8;
    
    break;
    
    // Hood
    case "hood": 
    sprite_index = spr_Hol_GetUp;
    image_speed = 1/16;
    
    break;
    
    // Idle
    case "idle": 
    sprite_index = spr_Hol_Idle;
    image_speed = 1/16;
    
    break;
    
    // Shoot
    case "shoot": 
    scr_Hol_Shoot();
    
    break;
    
    // Charging
    case "charging": 
    scr_Hol_Shoot();
    
    break;
    
    // Run
    case "run": 
    scr_Hol_Run();
    
    break;
    
    // Jump
    case "jump": 
    scr_Hol_Jump();
    
    break;
    
    // Fall
    case "fall": 
    scr_Hol_Fall();
    
    break;
    
    // Hit
    case "hit": 
    scr_Hol_Hit();
    
    break;
    
    // Hurt
    case "hurt": 
    
    sprite_index = spr_Hol_Hurt;
    
    
    break;
    
    // Laying
    case "laying": 
    
    sprite_index = spr_Hol_Laying;
    
    
    break;
    
    // Down
    case "down": 
    
    sprite_index = spr_Hol_Down;
    image_speed = 1/32;
    
    break;
    
    // Dying
    case "dying": 
    
    if dying = 0
    {
        audio_play_sound(snd_Hol_Dying,10,0);
        audio_play_sound(snd_Hol_Dying_2,10,0);
        audio_play_sound(snd_Hol_Dying_2,10,0);
        audio_play_sound(snd_Hol_Dying_2,10,0);
        audio_play_sound(snd_Hol_Dying_2,10,0);
        audio_play_sound(snd_Hol_Dying_2,10,0);
        part_particles_create(global.particle_system, x, y, global.pt_HolDying, 50);
        dying = 1;
    }
    sprite_index = spr_Hol_Dying;
    image_speed = 1/4;
    
    break;
    
    // Land
    case "land": 
    
    scr_Hol_Land();
    
    break;

}    
</string>
          </argument>
