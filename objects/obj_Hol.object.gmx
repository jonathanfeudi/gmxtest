            <kind>1</kind>
            <string>/// Event Keys from Text Counter

// Get Up and Change Dialogue Script

if global.textnextcounter = 9
and sprite_index = spr_Hol_Sitting
{
    image_index = 0;
    state = "hood";
    // Oenn
    obj_Oenn_Body.image_index = 0;
    obj_Oenn_Body.state = states.standingup;
    
    // Birds
    
    with obj_Bird
    {
        state = 'fly';
        image_xscale = choose(1,-1);
    }
    
    obj_Lighting.sunon = 1;
    
    audio_play_sound(snd_CoCo_Scream,10,0);
    
    audio_stop_sound(snd_CampFire);

    audio_play_sound(snd_Orro_Fight,10,1);
    
    obj_Camera.intro_room = 0;
    //obj_Oenn_Body.can_move = 1;
}

if (global.textnextcounter = 10
and sprite_index = spr_Hol_Idle
and obj_Camera_Point.target = 0) 
and room = room_IntroStage_00
//!instance_exists(obj_Camera_Point))
{
    //audio_play_sound(snd_Stamina_Tank,10,0);
    //obj_CoCo.run = 1;
    text_reset_state(0);
    obj_CoCo.timeline_index = tl_CoCo_Intro;
    obj_CoCo.timeline_position = 0;
    obj_CoCo.timeline_running = true;
    obj_CoCo.timeline_loop = false;
    /*with instance_create(obj_CoCo.x,obj_CoCo.y,obj_Camera_Point)
    {
        target = obj_CoCo;
    }*/
    
    obj_Camera.center = 0;
    
    obj_Camera_Point.x = obj_CoCo.x;
    obj_Camera_Point.x = obj_CoCo.y;
    obj_Camera_Point.target = obj_CoCo;
    
    /*if room = room_IntroStage_00
    {
        global.cut_scene = 1;
    }*/
}
    //can_move = 1;
    //lock = 0;
if global.textnextcounter = 16
and sprite_index = spr_Hol_Idle
and room = room_IntroStage_00
{
    global.textnextcounter = 0
    timeline_index = tl_Hol_Intro;
    timeline_position = 0;
    timeline_running = true;
    timeline_loop = false;
}


/////////////////// ORRO Fight

if global.textnextcounter = 5
and room = room_IntroStage_3
and hurt = 0
{
    global.textnextcounter = 0
    obj_Orro_Fight_Hol.image_index = 0;
    obj_Orro_Fight_Hol.image_speed = animation;
    obj_Orro_Fight_Hol.state = 'getup';
    /// Orro Starts next Timeline
}

if global.textnextcounter = 5
and room = room_IntroStage_3
and hurt = 1
{
    global.textnextcounter = 0
    //audio_play_sound(snd_Double_Jump_Item,10,0);
    //obj_Camera_Point.target = par_Oenn;
    
    //obj_Camera_Point.target = obj_Oenn_Body;
    /*with obj_Camera_Point
    {
        //instance_destroy();
        target = obj_Oenn_Body;
    }*/
    
    obj_Camera_Point.target = 0; /// LOCKS CAMERA to POSITION
    
    alarm[1] = room_speed*1;
    obj_Oenn_Body.timeline_index = tl_Oenn_Intro_4;
    obj_Oenn_Body.timeline_position = 0;
    obj_Oenn_Body.timeline_running = true;
    obj_Oenn_Body.timeline_loop = false;
    depth = -1;
    obj_Oenn_Body.depth = -4;
    obj_Oenn_Legs.depth = -3;
}

/////////////////// Death at Affliction

if global.textnextcounter = 5
and room = room_Affliction_1
{
    global.textnextcounter = 0
    //audio_play_sound(snd_Double_Jump_Item,10,0);
    //obj_Camera_Point.target = par_Oenn;
    /*with obj_Camera_Point
    {
        instance_destroy();
    }*/
    alarm[2] = room_speed*2; // Death State
}

